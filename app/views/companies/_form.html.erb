<%= render('layouts/errors', :object => @company) %>

<%= form_for [@type, @company] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :website %>
  <%= f.text_field :website %>

  <%= f.label :hours %>
  <%= f.text_field :hours %>

  <%= f.label :picture %>
  <%= f.text_field :picture %>
  <p><%= f.submit %></p>
<% end %>
